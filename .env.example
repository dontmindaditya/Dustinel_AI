PORT=5000
NODE_ENV=development
AUTH_PROVIDER=mock

# Azure AD B2C (required when AUTH_PROVIDER=azure)
AZURE_AD_B2C_TENANT=example.b2clogin.com
AZURE_AD_B2C_TENANT_ID=example.onmicrosoft.com
AZURE_AD_B2C_CLIENT_ID=dev-client-id
AZURE_AD_B2C_USER_FLOW=B2C_1_signupsignin

# Azure AI Vision
AZURE_VISION_ENDPOINT=https://example.cognitiveservices.azure.com
AZURE_VISION_KEY=dev-vision-key
VISION_PROVIDER=hybrid

# Optional research model endpoints (recommended for paper-derived models)
AZURE_PPE_MODEL_ENDPOINT=
AZURE_PPE_MODEL_API_KEY=
AZURE_DUST_MODEL_ENDPOINT=
AZURE_DUST_MODEL_API_KEY=

# Azure Blob Storage
AZURE_STORAGE_ACCOUNT_NAME=devstorageaccount1
AZURE_STORAGE_CONNECTION_STRING=UseDevelopmentStorage=true
AZURE_STORAGE_CONTAINER_NAME=safeguard-images

# Azure Cosmos DB
COSMOS_ENDPOINT=https://localhost:8081
COSMOS_KEY=dev-cosmos-key
COSMOS_DATABASE_NAME=safeguardai

# Azure ML
AZURE_ML_ENDPOINT=https://example.inference.ml.azure.com/score
AZURE_ML_API_KEY=dev-ml-key

# Azure Notification Hubs
NOTIFICATION_HUB_CONNECTION_STRING=Endpoint=sb://example/
NOTIFICATION_HUB_NAME=safeguard-notifications

# Azure Communication Services
AZURE_COMMUNICATION_CONNECTION_STRING=endpoint=https://example.communication.azure.com/;accesskey=dev
AZURE_COMMUNICATION_SENDER_EMAIL=noreply@safeguard.local
AZURE_COMMUNICATION_SENDER_PHONE=

# Optional
REDIS_URL=
ALLOWED_ORIGINS=http://localhost:3000
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100
CHECKIN_RATE_LIMIT_MAX=10
